@model IEnumerable<PracticeSession>

@{
    ViewData["Title"] = "Index";
    Layout = "_AdminLayout";

}

<section class="content">
    <p>
        <a asp-action="Create" class="btn btn-primary">Create New practice Session</a>
    </p>
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Coach List</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <table class="table table-bordered">
                <tr>
                    <th>Team_id</th>
                    <th>Coach_id</th>
                    <th>Address</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayNameFor(model => model.Team_id)
                        </td>
                        <td>
                            @Html.DisplayNameFor(model => model.Coach_id)
                        </td>
                        <td>
                            @Html.DisplayNameFor(model => model.Address)
                        </td>
                      <td>

                     <div class="btn-group" role="group">
                                        <a asp-action="Edit" class=" btn btn-primary" asp-route-id="@item.Id">Edit</a> |
                                        <a asp-action="Details" class="btn btn-info" asp-route-id="@item.Id">Details</a> |
                                        <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.Id">Delete</a>
                                    </div>
                                </td>
                            </tr>
                        }

                </table>
            </div>
            <!-- /.box-body -->
            <div class="box-footer clearfix">
                <!-- <ul class="pagination pagination-sm no-margin pull-right">
                    <li><a href="#">&laquo;</a></li>
                    <li><a href="#">1</a></li>
                    <li><a href="#">2</a></li>
                    <li><a href="#">3</a></li>
                    <li><a href="#">&raquo;</a></li>
                </ul>-->
            </div>
        </div>
        <!-- /.box -->




    </section>
